namespace caremonger.caremonger_db;

context data {
	entity t_incoming_information {
		KEY "ORDER_ID": Integer generated by default as identity ( start with 1 increment by 1 ); "REQUESTER_NAME":String(50); "REQUESTER_PHONE_NUMBER":Integer64 NOT NULL; "REQUEST_DESCRIPTION":String(5000);
		"REQUEST_TYPE":String(100) NOT NULL; "REQUESTER_LOCATION":String(50) NOT NULL; "REQUEST_TIME":UTCDateTime NOT NULL; 
		"CRITICALITY":String(20) NOT NULL; "REQUEST_STATUS":String(30) NOT NULL; "PROCESSOR_TEXT": String(5000); 
	};
};
