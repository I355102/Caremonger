<mvc:View controllerName="caremonger.caremonger_ui.controller.Object" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:semantic="sap.f.semantic" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<semantic:SemanticPage id="page" headerPinnable="false" toggleHeaderOnTitleClick="false" busy="{objectView>/busy}"
		busyIndicatorDelay="{objectView>/delay}">
		<semantic:titleHeading>
			<Title text="Lokesh Rajak"/>
		</semantic:titleHeading>
		<semantic:headerContent>
			<FlexBox alignItems="Start" justifyContent="SpaceBetween">
				<Panel backgroundDesign="Transparent">
					<ObjectAttribute title="Achievements" text="Bronze"/>
					<ObjectAttribute title="Helped" text="12 People"/>
				</Panel>
				<Panel backgroundDesign="Transparent">
					<ObjectNumber id="objectHeader" unit="Urgency Level of Task" class="sapUiSmallMarginBottom" state="Error" number="90 %"></ObjectNumber>
					<ObjectStatus class="sapUiSmallMarginBottom" text="Task is Critical" icon="sap-icon://status-positive" state="Error"/> 
					<ProgressIndicator width="300px" class="sapUiSmallMarginBottom" percentValue="90" displayValue="90 %" showValue="true" state="Error"/>
				</Panel>
			</FlexBox>
		</semantic:headerContent>
		<semantic:content>
			<l:VerticalLayout width="100%">
				<!--<Panel backgroundDesign="Transparent">-->
				<!--	<ObjectNumber id="objectHeader" unit="Urgency Level" class="sapUiSmallMarginBottom" state="Success" number="50 %"></ObjectNumber>-->
				<!--	<ObjectStatus class="sapUiSmallMarginBottom" text="Not So Critical" icon="sap-icon://status-positive" state="Success"/>-->
					<!--<ObjectStatus text="{i18n>ObjectDiscontinuedStatusText}" state="Error" visible="{path:'Discontinued'}"/>--> 
				<!--	<ProgressIndicator width="300px" class="sapUiSmallMarginBottom" percentValue="50" displayValue="50 %" showValue="true" state="Success"/>-->
				<!--</Panel> -->
				<Panel backgroundDesign="Transparent" headerText="{Requester Details}">
					<content>
				 
						<form:SimpleForm minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3"
							emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1">
							<form:content>
								<Label text="{i18n>tableColumnTitle1}"/>
								<Text text="Sree"/>
								<Label text="{i18n>tableColumnTitle6}"/>
								<Text text="{TableData>REQUESTER_PHONE_NUMBER}"/>
								<HBox alignItems="Center">
									<Label text="Processing Started" class="sapUiTinyMarginEnd"/>
									<Switch type="AcceptReject" state="true" change="toggleEnabled"/>
								</HBox>
								<!--<Select forceSelection="false" selectedKey="{/SelectedProduct}" type="IconOnly" icon="sap-icon://filter" autoAdjustWidth="true"-->
								<!--	items="{ path: '/ProductCollection', sorter: { path: 'Name' } }">-->
								<!--	<core:Item key="{ProductId}" text="{Name}"/>-->
								<!--</Select>-->
								<!--<Input id="productInput" type="Text" value="Astro Phone 6" placeholder="Enter Product ..." showValueHelp="true" valueHelpOnly="true"-->
								<!--	valueHelpRequest="handleValueHelp" class="sapUiSmallMarginBottom" width="15rem"/>-->
							</form:content>
						</form:SimpleForm>
					 
					</content>
				</Panel>
				<Panel backgroundDesign="Transparent" headerText="Comments">
					<content>
						<FeedInput post=".onPost"/>
						<!--<List id="idCommentsList" noDataText="No Comments" showSeparators="Inner" items="{/EntryCollection}" >-->
						<List id="idCommentsList" noDataText="No Comments" showSeparators="Inner">
							<FeedListItem info="{Type}" text="{Text}" convertLinksToAnchorTags="All"/>
						</List>
					</content>
				</Panel>
			</l:VerticalLayout>
		</semantic:content>
	</semantic:SemanticPage>
</mvc:View>
